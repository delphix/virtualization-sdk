{
  "type": "object",
  "properties":  {
      "id":  {
          "type": "string"
      },
      "name":  {
          "type": "string"
      },
      "version":  {
          "type": "string",
          "pattern": "^([0-9]+)\\.([0-9]+)\\.([a-zA-Z0-9_]+)$"
      },
      "hostTypes":  {
          "type": "array",
          "items":  {
              "type": "string",
              "enum": [ "UNIX", "WINDOWS" ]
          },
          "maxItems": 2
      },
      "entryPoint":  {
          "type": "string",
          "pattern": "^[^:]+:[^:]+$"
      },
      "srcDir":  {
          "type": "string"
      },
      "schemaFile":  {
          "type": "string"
      },
      "manualDiscovery":  {
          "type": "boolean"
      },
      "pluginType":  {
          "type": "string",
          "enum": ["DIRECT", "STAGED"]
      },
      "language":  {
          "type": "string",
          "enum": ["PYTHON27"]
      },
      "rootSquashEnabled":  {
          "type": "boolean"
      },
      "defaultLocale":  {
          "type": "string",
          "default": "en-us"
      }
    },
    "additionalProperties": false,
    "required": ["id", "name", "version", "hostTypes", "entryPoint", "srcDir", "schemaFile", "pluginType", "language"]
}
